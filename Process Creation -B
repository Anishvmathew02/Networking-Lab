#include<stdio.h>
#include<stdlib.h>
#include<unistd.h>
#include<sys/types.h>
#include<sys/wait.h>

 int main(){
   pid_t pid;
   pid=fork();

   if(pid<0){
     printf("Failed");
   }
   else if(pid==0){
     printf("\nChild Process Running");
     printf("\nChild Id :");
     printf("%d",getpid());
     printf("\nParent Id : %d\n",getppid());
   }
   else{
     wait(NULL);
     printf("\nParent Process Running");
     printf("\nParent Id : %d\n",getpid());
     char *args[]={"ps",NULL};
     execvp(args[0],args);
     exit(0);
   }
   return 0;
}
